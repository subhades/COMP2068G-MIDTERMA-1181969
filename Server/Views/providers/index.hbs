<h1>Providers Library</h1>
<a href = "/providers/create">Add New Provider</a>
    <ul class = "row card - group">
        {{#each providers}}
        <li class = "card m-4 text-center col-lg-4">
            <h5>{{this.name}}</h5>
        
        <a href = "/providers/edit/{{this._id}}" class="btn btn-primary">
        Edit</a>
    </li>
    {{/each}}
</ul>